{
    "name": "makinacorpus/profiling",
    "description": "Lightweight and flexible profiling toolbox, using the high resolution timer.",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Makina Corpus",
            "homepage": "https://makina-corpus.com/"
        },
        {
            "name": "Pierre RINEAU",
            "email" : "pierre.rineau@makina-corpus.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">= 8.0"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2",
        "phpunit/phpunit": "^9.2",
        "sentry/sentry": "^3.8",
        "symfony/config": "^5",
        "symfony/dependency-injection": "^5",
        "symfony/http-client": "^6.1",
        "symfony/stopwatch": "^6.1",
        "symfony/yaml": "^5"
    },
    "autoload": {
        "files": [
            "compat.php"
        ],
        "psr-4": {
            "MakinaCorpus\\Profiling\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
             "MakinaCorpus\\Profiling\\Tests\\": "tests/"
        }
    }
}
